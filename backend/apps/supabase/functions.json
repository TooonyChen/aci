            },
            "metadata_url": {
              "type": "string",
              "description": "Metadata URL"
            },
            "domains": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "Domains"
            },
            "attribute_mapping": {
              "type": "object",
              "description": "Attribute mapping",
              "properties": {
                "keys": {
                  "type": "object",
                  "properties": {},
                  "required": [],
                  "visible": [],
                  "additionalProperties": {
                    "type": "object",
                    "properties": {
                      "default": {
                        "oneOf": [
                          {"type": "object"},
                          {"type": "number"},
                          {"type": "string"},
                          {"type": "boolean"}
                        ]
                      },
                      "name": {
                        "type": "string"
                      },
                      "names": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      },
                      "array": {
                        "type": "boolean"
                      }
                    },
                    "required": ["name"]
                  }
                }
              },
              "required": ["keys"],
              "visible": [],
              "additionalProperties": false
            }
          },
          "required": ["type"],
          "visible": ["type", "metadata_xml", "metadata_url", "domains"],
          "additionalProperties": false
        }
      },
      "required": ["path", "body"],
      "visible": ["path", "body"],
      "additionalProperties": false
    }
